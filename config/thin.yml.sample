# parametrage des instances de thin.
# creer les repartoires dans /var/log et /var/run appartenant a l'utilisateur qui lance les serveurs
user: erasme
group: erasme
pid: /var/run/service-laclasse-startbox/thin.pid
log: /var/log/service-laclasse-startbox/thin.log
timeout: 30
max_conns: 1024
port: 9292
max_persistent_conns: 512
environment: production
adapter: rack
servers: 2 
daemonize: true
tag: service-laclasse-startbox

